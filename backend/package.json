{
  "name": "checkwho-backend",
  "scripts": {
    "lint": "eslint --ext ts,tsx,js,jsx .",
    "lint:fix": "eslint --fix --ext ts,tsx,js,jsx .",
    "prebuild": "rimraf build",
    "build": "nest build",
    "start": "node -r tsconfig-paths/register build/index",
    "dev": "concurrently \"npm run --prefix ../shared dev\" \"nest start --watch\"",
    "dev:debug": "concurrently \"npm run --prefix ../shared dev\" \"nest start --watch --debug\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "pre-commit": "lint-staged",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/database/datasource.ts",
    "migration:run": "npm run typeorm migration:run",
    "migration:gen": "npm run typeorm migration:generate",
    "migration:revert": "npm run typeorm migration:revert",
    "seed": "ts-node -r tsconfig-paths/register src/database/seeds/seed.ts",
    "generate-admin-key": "ts-node -r tsconfig-paths/register src/scripts/generate-admin-key.ts"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "3.37.0",
    "@aws-sdk/client-ssm": "^3.58.0",
    "@nestjs/cli": "^8.1.2",
    "@nestjs/common": "8.1.1",
    "@nestjs/core": "8.1.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "8.4.4",
    "@nestjs/sequelize": "8.0.0",
    "@nestjs/serve-static": "2.2.2",
    "@nestjs/terminus": "8.0.1",
    "@nestjs/typeorm": "8.1.4",
    "@ntegral/nestjs-sentry": "^3.0.7",
    "@sentry/node": "^6.19.7",
    "@sentry/tracing": "^7.0.0",
    "axios": "^0.26.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.1",
    "connect-typeorm": "2.0.0",
    "convict": "6.2.2",
    "dotenv": "10.0.0",
    "express": "^4.18.1",
    "express-session": "1.17.2",
    "helmet": "4.6.0",
    "jose": "^4.6.0",
    "lodash": "^4.17.21",
    "morgan": "1.10.0",
    "nodemailer": "6.7.3",
    "pg": "8.7.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.4.0",
    "typeorm": "0.3.7",
    "typeorm-naming-strategies": "2.0.0",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.2",
    "@nestjs/schematics": "^8.0.3",
    "@nestjs/testing": "^8.4.4",
    "@types/bcrypt": "^5.0.0",
    "@types/convict": "^6.1.1",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.181",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.24",
    "@types/nodemailer": "^6.4.4",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "concurrently": "^7.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.4",
    "jest": "^27.3.0",
    "lint-staged": "^11.2.3",
    "prettier": "2.7.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.3.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  },
  "lint-staged": {
    "**/*.(js|jsx|ts|tsx)": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{ts,tsx,js,jsx}": "eslint --cache --fix"
  }
}
